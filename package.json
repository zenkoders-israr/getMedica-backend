{
  "name": "nest-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "precommit": "npx eslint '**/*.ts'",
    "build": "nest build",
    "build:dev": "node --max-old-space-size=4096 ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:cron": "yarn build && node dist/cron.js",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node src/app/seeders/seed-run.ts",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "db:diagram": "typeorm-uml ormconfig.json",
    "migration:run": "npx typeorm -d ./dist/app/config/migration-datasource.config.js migration:run",
    "migration:revert": "npx typeorm -d ./dist/app/config/migration-datasource.config.js migration:revert",
    "migration:create": "npx typeorm migration:create ./src/app/migrations/$npm_config_name"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.668.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/bunyan": "^1.8.11",
    "@types/crypto-js": "^4.2.2",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1691.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.8",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "node-html-parser": "^7.0.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "stripe": "^16.11.0",
    "twilio": "^5.4.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "eslint/eslintrc",
    "@eslint/js": "^9.8.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^15.8.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-uml": "^1.6.5",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
